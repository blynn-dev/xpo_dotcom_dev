{
  "name": "XPO.COM DEV",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The official XPO.Dot.Com Developer Extension",
  "content_scripts": [{
    "matches": ["http://*.xpo.com/*"],
    "js": ["scripts/jquery.min.js","scripts/selection.js","scripts/dev.js"],
    "css": ["styles/dev.css"]
  }],
  "permissions": ["contentSettings", "tabs", "http://*.xpo.com/*"],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "16.png",
    "default_title": "XPO Site",
    "default_popup": "index.html"
  }
}